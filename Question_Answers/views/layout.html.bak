<html xmlns="http://www.w3.org/1999/xhtml">
 <head>
  <title>
   {{=response.title or request.application}}
  </title>
  {{response.files.append(URL(request.application,'static','plugin_layouts/layouts/RedCity/style.css'))}}
  {{response.files.append(URL('static','plugin_layouts/superfish.js'))}}
  {{include 'web2py_ajax.html'}}
  <style>
body {   background:  url("http://127.0.0.1:8000/stack/static/bg") no-repeat center;
}
ul.web2py-menu-vertical { width: 150px; position: relative; top:1em; left:1em;  z-index:30; border:0; margin:0; padding: 0; list-style: none; text-decoration: none;}
ul.web2py-menu-vertical li { margin:0; padding:0; border:0; width: 150px; background: black; text-align:left;}
ul.web2py-menu-vertical li a { margin:0; padding: 1px 3px 1px 3px; border:0; width: 144px; color: white; font-size:12px;}
div.flash { position: fixed; float: right; padding: 10px; top: 10px; right: 30px; opacity: 0.75; margin: 10px 10px 10px 10px; text-align: center; clear: both; color: #fff; font-size: 11pt; text-align: center; vertical-align: middle; cursor: pointer; background: black; border: 2px solid #fff; -moz-border-radius: 5px; -webkit-border-radius: 5px; z-index: 2; }
div.error { background-color: red; color: white; padding: 3px; }
.auth_navbar { color:blue; text-align:right; z-index:20; position: absolute; top: 2px; right:30px; border:0; padding:0 }
     </style>
 </head>
 <body>
  <!-- start header -->
  {{try:}}{{=auth.navbar(action=URL('default','user'))}}{{except:pass}}

  <div id="container">
   <!-- header -->
   <div id="header" >
    <div id="logo">
     <a href="http://www.iiits.ac.in/">
      QUESTION-WARE OF IIIT-SRICITY
     </a>
    </div>
    <div id="menu">
     {{=MENU(response.menu,_class='web2py-menu')}}
    <script>
      jQuery(document).ready(function(){jQuery('ul.web2py-menu').superfish({delay:400});});
    </script>
    </div>
   </div>
      <img src="http://127.0.0.1:8000/stack/static/ii">
      <img src="http://127.0.0.1:8000/stack/static/logo" align="right">
      <a href="https://www.facebook.com"><img src="http://127.0.0.1:8000/stack/static/fb" height="5%" width="5%" align="right"></a> 
   <!--end header -->
       <iframe src="http://www.facebook.com/plugins/like.php?href=http%3A%2F%2Fhttp://127.0.0.1:8000/stack/default/show/4&amp;layout=standard&amp;show_faces=true&amp;action=like&amp;colorscheme=light&amp" style="overflow:hidden;width:100%;height:80px;" scrolling="no" frameborder="0" allowTransparency="true"><a href="http://www.trivoo.net" class="fbook" align="center"></a></iframe>
      <!-- main -->
   <div id="main">
    <div id="content">
     <div class="flash">{{=response.flash or ''}}</div>
     {{include}}
    </div>
   </div>
   <!-- end main -->
   <!-- footer -->
   <div id="footer">
    <div id="left_footer">
     2014 Â© Copyrights Reserved, IIIT SriCity
    </div>
    <div id="right_footer">
     Design by
      kishan,shyam and ashutosh
     </a>
     <!-- ****************************************** -->
    </div>
   </div>
   <!-- end footer -->
  </div>
 </body>
</html>
